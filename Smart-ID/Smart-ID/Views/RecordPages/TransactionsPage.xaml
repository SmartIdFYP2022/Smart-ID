<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:Smart_ID.ViewModels.RecordPagesViewModels"
             x:Class="Smart_ID.Views.RecordPages.TransactionsPage"
             Title="TransactionsPage">
    <ContentPage.BindingContext>
        <vm:TransactionsPageViewModel/>
    </ContentPage.BindingContext>
    <Grid>
        <Button Text="LOG IN" TextColor="#0d1332" BackgroundColor="#a9c6df"
                            HeightRequest="45" Margin="30,20,30,0" CornerRadius="50" FontAttributes="Bold"
                            Command="{Binding btnDashboard}"/>
       
    </Grid>
</ContentPage>